{
    "manifest_version": 3,
    "name": "AI Image Detector",
    "version": "1.0",
    "description": "Detect if an image is AI-generated",
    "permissions": ["storage", "activeTab", "scripting"],
    "host_permissions": ["http://127.0.0.1:5000/*"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    }
  }
  